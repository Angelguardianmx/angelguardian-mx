- unless @posts.empty?
  - radio = ["especial","weather", "dod", "police-dod", "quote"].include?(@type) ? true : false
  - if radio
    - type = @type
  - else
    - type = "none"
  $(".search-news##{@type} table tbody").html("#{escape_javascript(render partial: 'admin/portadas/search_res', collection: @posts, as: 'post', locals: { radio: radio, type: type })}")
  $( ".draggable" ).draggable({ connectToSortable: ".droppable", helper: "clone", revert: "invalid" });
